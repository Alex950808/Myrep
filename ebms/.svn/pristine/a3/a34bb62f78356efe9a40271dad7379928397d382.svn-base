        var flag = "@ViewData["flag"]";
        if (flag == "2") {
            $('#order').hide();
        }
        else {
            $('#freight').hide();
        }
    });
    var only = 0;
    function fun1() {
        if (only == 0) {
            var flag = "@ViewData["flag"]";
                 form1 = document.getElementById("form1");

                 form1.action = "/AliExpressOrder/ImportAliExcel";
                 document.getElementById("form1").submit();
                 loading(0);
                only = 1;
            }
        };
</script>
