/*审核列表*/
.lists li{width:100%;border-bottom:1px solid #eee;display:table;height:0.5rem;padding:0.1rem 0;}
 .remark{float:right;margin:0.1rem 0.08rem}
.lists li h3{color:#333;font-style:normal;font-weight:normal;font-size:0.14rem;margin:0 0 0 0.08rem}
.lists li h4{color:#666;font-style:normal;font-weight:normal;font-size:0.12rem;margin:0.1rem 0 0 0.08rem}   
.l-box{float:left}   
.header-bar {line-height:0.4rem;height:0.4rem;padding:0 0.1px;border-bottom:1px solid #ccc;box-shadow:0 0 10px #ddd}
.back-btn {color:#666}
.r-trigger-list-status {float:right;margin-right:0.1rem}
.mask-box {position:fixed;top:0;left:0;background:rgba(0,0,0,0);width:100%;height:100%;display:none;z-index:9999999999999}
.select-status {background:#fff;position:absolute;right:0;top:0.4rem;padding:2px 10px;border:1px solid #ddd}
.select-status li {color:#666;padding:0.08rem 0}
/*搜索*/
 .search-box {position:fixed;z-index:999;bottom:0;left:0;width:100%;height:50px;border-top:1px solid #ddd;background:#fff}
.list-page {padding-bottom:50px;}
.flex-box {display:flex;flex-wrap:nowrap}
.search-btn {width:80px;height:100px;display:block;background:#2299ee;color:#fff;text-align:center;line-height:50px}
.search-grow{flex-grow:1}
.search-text {width:100%;height:100%;padding:0 10px;border:0;outline:0;line-height:50px;height:50px}
.search-item .icon-search {font-size:20px;line-height:50px;margin-left:10px;height:50px}









