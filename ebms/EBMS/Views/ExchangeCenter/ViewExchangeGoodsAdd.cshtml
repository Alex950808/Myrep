                                    $("#AddressMessage").val(data.ModelList["AddressMessage"]);
                                    $("#SingleTime").val(data.ModelList["SingleTime"]);
                                    $("#StoreCode").val(data.ModelList["StoreCode"]);
                                    $("#SystemRemark").val(data.ModelList["SystemRemark"]);
                                    grid.datagrid('loadData', { total: 0, rows: [] });
                                    if ($("#SystemRemark").val() != "")
                                        $("#SystemRemark").css("color", "#d02e2e")
                                    $(data.Json.rows).each(function (i, item) {
                                        $('#List').datagrid('appendRow', { SendProductCode: item.SendProductCode, SendProductName: item.SendProductName, SendProductNum: item.SendProductNum })
                                    });
                                } else
                                    window.location.reload();
                            });
                        } else {
                            $("#OrderCode").val(data.ModelList["OrderCode"]);
                            $("#VipName").val(data.ModelList["VipName"]);
                            $("#StoreName").val(data.ModelList["StoreName"]);
                            $("#ReceivingName").val(data.ModelList["ReceivingName"]);
                            $("#NeedPostalCode").val(data.ModelList["NeedPostalCode"]);
                            $("#OrderScode").val(data.ModelList["OrderScode"]);
                            $("#ReceivingPhone").val(data.ModelList["ReceivingPhone"]);
                            $("#ReceivingTelPhone").val(data.ModelList["ReceivingTelPhone"]);
                            $("#VipCode").val(data.ModelList["VipCode"]);
                            $("#ReceivingAddress").val(data.ModelList["ReceivingAddress"]);
                            $("#AddressMessage").val(data.ModelList["AddressMessage"]);
                            $("#SingleTime").val(data.ModelList["SingleTime"]);
                            $("#StoreCode").val(data.ModelList["StoreCode"]);
                            $("#SystemRemark").val(data.ModelList["SystemRemark"]);
                            grid.datagrid('loadData', { total: 0, rows: [] });
                            if ($("#SystemRemark").val() != "")
